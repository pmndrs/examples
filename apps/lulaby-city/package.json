{
  "name": "lulaby-city",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@pmndrs/branding": "^0.0.8",
    "@react-three/drei": "^9.107.0",
    "@react-three/fiber": "^8.16.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.165.0"
  },
  "scripts": {
    "dev": "vite --host --open",
    "build": "tsc && vite build",
    "build2": "tsc && vite-build $npm_package_name",
    "preview": "vite preview"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "tabWidth": 2,
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/three": "^0.165.0",
    "@vitejs/plugin-react": "^4.3.1",
    "husky": "^4.0.10",
    "lint-staged": "^15.2.7",
    "prettier": "^1.19.1",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "positional",
    "gltf",
    "audio"
  ],
  "description": "Panning around a city with positional audio.",
  "homepage": "https://codesandbox.io/s/gkfhr",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/pmndrs/examples/tree/main/apps/lulaby-city"
  }
}
